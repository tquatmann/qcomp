		},
        {
            "name": "B",
            "kind": "open",
            "description": "Magnitude of bounds for global rewards",
            "type": "positive integer"
        }
	],
	"properties": [
		{
			"name": "lazy",
			"description": "The tradeoff between the minimum time and energy spent.",
			"type": "multi-tradeoff"
		},	
		{
			"name": "max_value_bounded",
			"description": "The maximum probability of obtaining at least a given scientific value, while staying in the time and energy constraints.",
			"type": "prob-reach-reward-bounded"
		},
		{
			"name": "max_value_bounded",
			"description": "The maximum probability of obtaining any value without exceeding time or energy constraints.",
			"type": "multi-tradeoff"
		},
		{
			"name": "safe_collection",
			"description": "The maximum probability of obtaining at least a given scientific value, while staying in the time and energy constraints.",
			"type": "prob-reach"
		},
		{
			"name": "safe_collection_tradeoff",
			"description": "The tradeoff between maximising the probability of obtaining at least a given scientific value, and the maximum probability of staying in the time and energy constraints.",
			"type": "multi-tradeoff"
		}
